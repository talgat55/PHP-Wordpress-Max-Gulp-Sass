
# BEGIN WordPress
# Строки между `BEGIN WordPress` и `END WordPress` были созданы автоматически.
# Они могут быть изменены только фильтрами WordPress.
# Все изменения между этими отметками будут перезаписаны.
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
<IfModule mod_rewrite.c>
   Redirect 301 /catalog/ /
   Redirect 301 /catalog/index.php?option=com_content&task=view&id=80&Itemid=7 /about/
   Redirect 301 /catalog/index.php?option=com_content&task=view&id=491&Itemid=18 /news/
   Redirect 301 /catalog/index.php?option=com_contact&task=view&contact_id=1&Itemid=3 /kontakty/
   Redirect 301 /catalog/index.php?option=com_content&task=view&id=84&Itemid=11 /partner/
   Redirect 301 /catalog/index.php?option=com_content&task=view&id=87&Itemid=13 /archive/
 #  арт максимум   Redirect 301 /catalog/index.php?option=com_content&task=view&id=285&Itemid=17
 #  рейтинг статей   Redirect 301 /catalog/index.php?option=com_content&task=category&sectionid=1&id=1&Itemid=2

</IfModule>
